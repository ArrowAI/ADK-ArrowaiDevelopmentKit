{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "arrowai": {
            "root": "",
            "sourceRoot": "src",
            "projectType": "application",
            "prefix": "kt",
            "schematics": {
                "@schematics/angular:component": {
                    "style": "scss"
                }
            },
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist",
                        "index": "src/index.html",
                        "main": "src/main.ts",
                        "polyfills": "src/polyfills.ts",
                        "tsConfig": "src/tsconfig.app.json",
                        "assets": [
                            "src/favicon.ico",
                            "src/assets",
                            "src/element.json",
                            "src/.htaccess",
                            {
                                "glob": "**/*",
                                "input": "node_modules/ngx-monaco-editor/assets/monaco",
                                "output": "/assets/monaco"
                            },
                            "src/manifest.webmanifest"
                        ],
                        "styles": [
                            "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
                            "src/styles.scss",
                            "node_modules/bootstrap/dist/css/bootstrap.css",
                            "node_modules/perfect-scrollbar/css/perfect-scrollbar.css",
                            "node_modules/socicon/css/socicon.css",
                            "node_modules/@fortawesome/fontawesome-free/css/all.min.css",
                            "src/assets/plugins/line-awesome/css/line-awesome.css",
                            "src/assets/plugins/flaticon/flaticon.css",
                            "src/assets/plugins/flaticon2/flaticon.css",
                            "src/assets/sass/style.angular.scss",
                            "src/assets/plugins/custom/datatables/datatables.bundle.css",
                            "src/assets/sass/pages/todo/tasks.scss",
                            "src/assets/plugins/custom/jstree/jstree.bundle.css"
                        ],
                        "scripts": [
                            "node_modules/popper.js/dist/umd/popper.js",
                            "node_modules/tooltip.js/dist/umd/tooltip.min.js",
                            "node_modules/perfect-scrollbar/dist/perfect-scrollbar.js",
                            "node_modules/js-cookie/src/js.cookie.js",
                            "src/assets/js/global/layout/layout.js",
                            "src/assets/plugins/custom/jstree/jstree.bundle.js",
                            "src/assets/js/pages/components/extended/treeview.js"
                        ],
                        "stylePreprocessorOptions": {
                            "includePaths": [
                                "src/assets"
                            ]
                        },
                        "es5BrowserSupport": true
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [],
                            "scripts": [],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [{
                                "type": "initial",
                                "maximumWarning": "5mb",
                                "maximumError": "10mb"
                            }]

                        }
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "arrowai:build"
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "arrowai:build:production"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "arrowai:build"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "src/test.ts",
                        "polyfills": "src/polyfills.ts",
                        "tsConfig": "src/tsconfig.spec.json",
                        "karmaConfig": "src/karma.conf.js",
                        "styles": [
                            "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
                            "src/styles.scss"
                        ],
                        "scripts": [],
                        "assets": [
                            "src/assets",
                            {
                                "glob": "**/*",
                                "input": "node_modules/ngx-monaco-editor/assets/monaco",
                                "output": "/assets/monaco"
                            },
                            "src/manifest.webmanifest"
                        ]
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "src/tsconfig.app.json",
                            "src/tsconfig.spec.json"
                        ],
                        "exclude": [
                            "**/node_modules/**"
                        ]
                    }
                }
            }
        },
        "arrowai-e2e": {
            "root": "e2e/",
            "projectType": "application",
            "prefix": "",
            "architect": {
                "e2e": {
                    "builder": "@angular-devkit/build-angular:protractor",
                    "options": {
                        "protractorConfig": "e2e/protractor.conf.js",
                        "devServerTarget": "arrowai:serve"
                    },
                    "configurations": {
                        "production": {
                            "devServerTarget": "arrowai:serve:production"
                        }
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": "e2e/tsconfig.e2e.json",
                        "exclude": [
                            "**/node_modules/**"
                        ]
                    }
                }
            }
        }
    },
    "defaultProject": "arrowai",
    "cli": {
        "analytics": "99ac9b93-98cf-4103-88a7-032d50e496a4"
    }
}